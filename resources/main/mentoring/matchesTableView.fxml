<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableView?>

<SplitPane fx:id="parent" dividerPositions="0.5" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mentoring.view.datastructure.MatchesTableView">
   <items>
      <SplitPane fx:id="personPane" dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
        <items>
            <TableView fx:id="menteeTable" />
            <TableView fx:id="mentorTable" />
        </items>
      </SplitPane>
      <SplitPane fx:id="matchPane" dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
        <items>
            <TableView id="computed-table" fx:id="computedTable" />
            <TableView id="manual-table" fx:id="manualTable" />
        </items>
      </SplitPane>
   </items>
</SplitPane>
